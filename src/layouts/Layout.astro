---
import GA4 from '@/meta/GA4.astro';

interface Props {
	title: string;

}
const { title = 'Astro' } = Astro.props;
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<GA4 />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>

